{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "sindarin-libs",
  "version": "1.0.0",
  "description": "Pre-built static libraries for Sindarin projects",
  "supports": "!uwp",
  "dependencies": [
    "zlib",
    "yyjson",
    "json-c",
    {
      "name": "libxml2",
      "default-features": false,
      "features": ["zlib"]
    },
    "libyaml",
    "openssl",
    {
      "name": "ngtcp2",
      "features": ["openssl"]
    },
    {
      "name": "libssh",
      "default-features": false,
      "features": ["server"]
    },
    {
      "name": "libgit2",
      "default-features": false,
      "features": ["ssh", "openssl", "pcre2"]
    },
    {
      "name": "curl",
      "default-features": false,
      "features": ["ssl"]
    }
  ]
}
